<header class="main-header">Addressbook</header>

<nav>
    <header>
        Contacts
    </header>

    <ul class="contact-list">

        {{!-- create new contact --}}
        <li>
            - new contact -
        </li>    

        {{!-- contacts --}}
        {{#each contact in model}}
        <li class="contact-list-item">
            {{#link-to 'contacts.view' contact}}
                {{contact.displayName}}
            {{/link-to}}
        </li>
        {{/each}}

    </ul>    
</nav>

{{outlet}}
